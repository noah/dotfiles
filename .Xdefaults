urxvt*background:#1a1a1a
urxvt*foreground:white
urxvt*buffered:true
urxvt*transparent: 0
urxvt*depth:32
urxvt*jumpScroll:true
urxvt*loginShell:true
urxvt*saveLines:65535
urxvt*termName: rxvt-256color

xft.dpi:        96
xft.antialias:  true
xft.rgba:       rgb
xft.hinting:    true
xft.hinstyle:   hintslight

urxvt*scrollBar_right:false
urxvt*scrollBar:false
urxvt*borderless:false

urxvt*inheritPixmap: false
urxvt*tintColor: #FFFFFF
urxvt*shading: 90

urxvt*font:xft:envy code r:medium:pixelsize=13
urxvt*boldFont:xft:envy code r:medium:pixelsize=13

#include "/home/noah/.colors/dark"
!#include "/home/noah/.colors/bright"


urxvt.perl-ext:           default,matcher
urxvt.urlLauncher:        /usr/bin/google-chrome
urxvt.keysym.C-Delete:    perl:matcher
urxvt.underlineURLs:      true

urxvt*scrollTtyOutput:      false
urxvt*scrollWithBuffer:     true
urxvt*scrollTtyKeypress:    true

Xcursor.theme: Vanilla-DMZ
